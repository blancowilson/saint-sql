      object daSQL3: TdaSQL
        GuidCollationType = gcMSSQLServer
        DatabaseName = 'SrvConnection'
        DatabaseType = dtMSSQLServer
        DataPipelineName = 'COMICOBRO'
        EditSQLAsText = True
        LinkColor = clGreen
        LinkType = ltParameterizedSQL
        MaxSQLFieldAliasLength = 0
        SQLText.Strings = (
          
            'select  fapag.codclie, fapag.Descrip, fapag.NumeroD, fapag.CodVe' +
            'nd, FechaP.ultimopago, fapag.FechaV, '
          '    datediff (DAY, fapag.FechaV,FechaP.ultimopago) Vencido'
          
            #9#9', sum((CASE WHEN pagos.TIPOCXC LIKE '#39'3%'#39' THEN -1 ELSE 1 END)*(' +
            'pagos.MONTO-pagos.MTOTAX)) AS MONTO '
          
            #9#9', sum((CASE WHEN pagos.TIPOCXC LIKE '#39'3%'#39' THEN -1 ELSE 1 END)*(' +
            'pagos.Comision)) AS COMISION'
          'from'#9
          #9'(SELECT'
          #9#9'fac.TipoCxc'
          #9#9', cl.CodVend'
          #9#9',cl.Descrip'
          #9#9', pg.NroRegi'
          #9#9', CC.codclie'
          #9#9', fac.NroUnico'
          #9#9', fac.FechaE'
          #9#9', fac.FechaV'
          #9#9', pg.NumeroD'
          #9#9'FROM SAACXC CC WITH (NOLOCK)'
          #9#9#9'INNER JOIN SACLIE CL '
          #9#9#9#9#9'ON (CL.CodClie=CC.CodClie)'
          #9#9#9'LEFT JOIN SAPAGCXC PG '
          #9#9#9#9'ON (PG.NROPPAL=CC.NROUNICO)'
          
            #9#9#9'inner join SAACXC fac on fac.NumeroD = pg.NumeroD and fac.Tip' +
            'oCxc = pg.tipocxc and fac.NroUnico = pg.NroRegi'
          #9#9'WHERE ((CC.TipoCxc Like '#39'4%'#39') Or (CC.Tipocxc Like '#39'3%'#39')) '
          #9#9#9'And (CC.EsReten=0) '
          #9#9#9'AND (CC.FromTran=1) AND CC.TipoCxc IN ('#39'31'#39','#39'41'#39')'
          
            #9#9#9'AND'#9' (CONVERT(DATETIME,:FECHAINI+'#39' 00:00:00'#39',120)<=PG.FECHAE)' +
            ' '
          #9#9#9'AND (PG.FECHAE<=CONVERT(DATETIME,:FECHAFIN+'#39' 23:59:59'#39',120))'
          #9#9#9'AND FAC.Saldo <=0'
          
            #9#9'group by pg.NroRegi, cc.CodClie,fac.FechaE, fac.TipoCxc, pg.Nu' +
            'meroD, fac.FechaV, cl.CodVend, cl.Descrip, fac.NroUnico) fapag '
          #9'inner join SAPAGCXC '
          
            #9#9'pagos on pagos.NumeroD = fapag.NumeroD  and pagos.NroRegi = fa' +
            'pag.NroRegi '
          
            #9'inner join ( SELECT max (fechae)  AS ultimopago, nroregi, Numer' +
            'oD,TipoCxc'
          #9#9#9#9'FROM SAPAGCXC'
          #9#9#9#9'where TipoCxc in (10,20)'
          #9#9#9#9'group by NroRegi,NumeroD,TipoCxc) fechap '
          
            #9#9#9'on pagos.NumeroD = fechap.NumeroD and pagos.NroRegi = fechap.' +
            'NroRegi'
          
            #9'group by fapag.CodClie,fapag.Descrip, fapag.NumeroD, fapag.CodV' +
            'end,fechap.ultimopago,  fapag.FechaV '
          '')
        SQLType = sqSQL2

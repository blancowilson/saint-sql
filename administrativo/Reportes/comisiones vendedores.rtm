      object daSQL4: TdaSQL
        GuidCollationType = gcMSSQLServer
        DatabaseName = 'SrvConnection'
        DatabaseType = dtMSSQLServer
        DataPipelineName = 'COMITABL'
        EditSQLAsText = True
        LinkColor = clPurple
        LinkType = ltParameterizedSQL
        MaxSQLFieldAliasLength = 0
        SQLText.Strings = (
          
            'SELECT V.CodVend, V.TipoRgo, V.Desde, V.Hasta, V.Porct, v.MontoA' +
            ', v.Comision'
          
            '  FROM (SELECT tr.CodVend CodVend, CV.TipoRgo TipoRgo, CV.Desde ' +
            'Desde, CV.Hasta Hasta, CV.Monto As Porct,'
          
            '               round((Case when (cv.desde<=sum(Tr.BASE)) And (Su' +
            'm(TR.BASE)<=cv.Hasta) Then Sum(TR.BASE) else 0 end),2) AS MontoA' +
            ','
          
            '               round((case when cv.tipoRgo in (4)   then -- Por ' +
            'porcentaje de utilidad'
          
            '                                (Case when (cv.desde<=sum(Tr.Mon' +
            'to)) And Sum(TR.Monto)<=cv.Hasta Then Sum(TR.BASE)*cv.monto/100 ' +
            'else 0 end)'
          
            '                           when cv.tipoRgo in (5,6) then -- Por ' +
            'Tabla de ventas o cobros'
          
            '                                (Case when (cv.desde<=sum(Tr.BAS' +
            'E )) And Sum(TR.BASE )<=cv.Hasta Then Sum(TR.BASE)*cv.monto/100 ' +
            'else 0 end)'
          
            '                                 when cv.tipoRgo in (7)   then -' +
            '- Por dias de cobranza'
          
            '                        (Case when (cv.desde<=sum(Tr.MONTO)) And' +
            ' Sum(TR.Monto)<=cv.Hasta Then'
          
            '                              Sum(TR.BASE)*cv.monto/100 else Sum' +
            '(TR.BASE)*cv.monto/100 end) '
          '              end),2) AS Comision'
          '         FROM (-- tabla de utilidad --'
          '               SELECT 4 AS Tipo, CODVEND, FECHAE'
          '                      ,SIGNO*Monto_Utili   AS BASE'
          '                      ,SIGNO*Monto_PorUtil AS MONTO'
          '                 FROM VW_ADM_FACTURAS'
          '                WHERE (Tipofac In ('#39'A'#39','#39'B'#39')) '
          '              -- tabla de ventas --'
          '                UNION ALL'
          '               SELECT 5 as Tipo, CODVEND, FECHAE'
          '                      ,Signo*monto AS BASE'
          '                      ,Signo*monto AS MONTO'
          '                 FROM VW_ADM_FACTURAS'
          '                WHERE (Tipofac In ('#39'A'#39','#39'B'#39')) '
          '              -- tabla de cobranzas --'
          '                UNION ALL'
          '               SELECT 6 As Tipo, CC.CODVEND, CC.FECHAE'
          '                      ,CC.monto AS BASE'
          '                      ,CC.monto AS MONTO'
          '                 FROM ('
          '                       SELECT FA.codvend, FA.fechae, '
          #9#9'                          FA.CONTADO AS BASE, '
          #9#9#9'                        FA.CONTADO AS MONTO'
          '                         FROM VW_ADM_FACTURAS FA'
          
            '                        WHERE (FA.Tipofac In ('#39'A'#39','#39'B'#39')) And (FA.' +
            'Contado<>0) '
          '                        UNION ALL  '
          '                       SELECT CC.CodVend, CC.FechaE, '
          
            #9#9'                          (CASE WHEN CC.TIPOCXC LIKE '#39'3%'#39' THEN' +
            ' -1 ELSE 1 END)*ISNULL(PG.MONTO-PG.MTOTAX,CC.MONTO-CC.MTOTAX) AS' +
            ' BASE, '
          
            '                              (CASE WHEN CC.TIPOCXC LIKE '#39'3%'#39' TH' +
            'EN -1 ELSE 1 END)*ISNULL(PG.MONTO-PG.MTOTAX+PG.RetenIVA+CancelI,' +
            'CC.MONTO-CC.MTOTAX) AS MONTO'
          '                         FROM SAACXC CC'
          #9#9'                          LEFT JOIN SAPAGCXC PG '
          #9#9'                                 ON (PG.NROPPAL=CC.NROUNICO) '
          
            '                        WHERE ((CC.TipoCxc Like '#39'4%'#39') Or (CC.Tip' +
            'ocxc Like '#39'3%'#39')) And '
          
            '                              (CC.EsReten=0) AND (CC.FromTran=1)' +
            ' '
          '                      ) CC'
          '              -- tabla por dias de cobro ---'
          '                UNION ALL'
          '               SELECT 7 as Tipo, CC.CODVEND, PG.FECHAE'
          
            '                     ,(CASE WHEN CC.TIPOCXC LIKE '#39'3%'#39' THEN -1 EL' +
            'SE 1 END)*ISNULL(PG.MONTO-PG.MTOTAX+PG.RetenIVA+CancelI,CC.MONTO' +
            '-CC.MTOTAX) AS BASE '
          
            '                     ,ISNULL(DATEDIFF(DD,PG.FECHAO,PG.FECHAE),0)' +
            ' AS MONTO'
          '                 FROM SAACXC CC'
          '                      LEFT JOIN SAPAGCXC PG '
          #9#9#9'                       ON PG.NroPpal=CC.NroUnico'
          
            '                WHERE ((CC.TipoCxc Like '#39'4%'#39') Or (CC.Tipocxc Lik' +
            'e '#39'3%'#39')) And '
          '                      (CC.FromTran=1)  And (CC.EsReten=0)) TR'
          '                      INNER JOIN SACVEN CV '
          
            #9#9#9'                        ON (cv.CodVend=tr.CodVend) and (tr.ti' +
            'po=cv.TipoRgo)'
          
            '        WHERE (CONVERT(DATETIME,:FECHAINI+'#39' 00:00:00'#39',120)<=TR.F' +
            'ECHAE) AND '
          
            '              (TR.FECHAE<=CONVERT(DATETIME,:FECHAFIN+'#39' 23:59:59'#39 +
            ',120)) AND '
          '              (CV.HASTA>CV.DESDE) AND (CV.Monto<>0)'
          
            '        group by tr.CodVend, CV.TipoRgo, cv.NroUnico, CV.DESDE, ' +
            'CV.Hasta, cv.monto) V'
          ' order by V.CODVEND, V.TipoRgo'
          ''
          '')
        SQLType = sqSQL2
        object daField1: TdaField
          Alias = 'CodVend'
          DisplayWidth = 10
          FieldAlias = 'CodVend'
          FieldLength = 10
          FieldName = 'CodVend'

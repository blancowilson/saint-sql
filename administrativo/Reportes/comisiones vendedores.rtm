        LinkColor = clMaroon
        LinkType = ltParameterizedSQL
        MaxSQLFieldAliasLength = 0
        SQLText.Strings = (
          
            'SELECT fa.codvend CodVend, fa.tipofac, fa.fechae, fa.numerod, fa' +
            '.CodClie, fa.Descrip,    '
          
            '       FA.SIGNO*fa.monto as monto, FA.SIGNO*fa.MtoComiVta as Com' +
            'ision,  '
          '       COALESCE(fa.MtoComiVta/Nullif(fa.Monto,0)*100,0) as porct'
          '  FROM SAFACT FA'
          ' WHERE --(fa.MtoComiVta+fa.MtoComiVtaD<>0) AND'
          '       (FA.TIPOFAC IN ('#39'A'#39','#39'B'#39'))'
          
            '       AND (SUBSTRING(FA.CODSUCU,1,LEN(:CODSUCU+'#39#39'))=:CODSUCU+'#39#39 +
            ')  '
          
            '       AND (CONVERT(DATETIME,:FECHAINI+'#39' 00:00:00'#39',120)<=fa.FECH' +
            'AE) '
          
            '       AND (fa.FECHAE<=CONVERT(DATETIME,:FECHAFIN+'#39' 23:59:59'#39',12' +
            '0))'
          ' ORDER BY fa.FechaE')
